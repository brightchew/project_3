<h1> log in </h1>
<%= form_tag new_session_path do %>
<div class="form-group">
<%= label_tag :email %>
<%= email_field_tag :email, params[:email], class: "form-control" %>
</div>
<div class="form-group">
<%= label_tag :password %>
<%= password_field_tag :password, nil, class: "form-control" %>
</div>
<%= submit_tag "log in", class: "btn-primary" %>
<% end %>

